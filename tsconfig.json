{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@components": [
        "components"
      ],
      "@store/*": [
        "store/*"
      ],
      "@store": [
        "store"
      ],
      "@app-types/*": [
        "types/*"
      ],
      "@app-types": [
        "types"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@constants": [
        "constants"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@utils": [
        "utils"
      ]
    }
  },
  "include": [
    "src"
  ]
}